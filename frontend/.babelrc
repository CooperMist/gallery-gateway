{
  "presets": [
    [
      "env", {
        "loose": true,
        "modules": "umd",
        "target": ["ie 10"],
        "useBuiltIns": "entry"
      }
    ],
    "stage-0",
    "react"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
