{
  "presets": [
    [
      "env", {
        "loose": true,
        "modules": false,
        "target": ["ie 10"],
        "useBuiltIns": "entry"
      }
    ],
    "stage-0",
    "react"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "env", {
            "loose": true,
            "modules": "commonjs",
            "target": ["ie 10"],
            "useBuiltIns": "entry"
          }
        ],
        "stage-0",
        "react"
      ]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
